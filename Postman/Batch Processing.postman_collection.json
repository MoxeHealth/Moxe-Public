						{
							"listen": "test",
							"script": {
								"exec": [
									"var jsonData = pm.response.json();",
									"",
									"pm.globals.set(\"smoke-test-token\", jsonData.data.token);",
									"",
									"pm.test(\"Status code is 200\", function () {",
									"    pm.response.to.have.status(200);",
									"});",
									"",
									"pm.test(\"Payor ORG/ENV/APP (m_prid) is UPH_PRD_HFF\", function() {",
									"    pm.expect(jsonData.data.m_prid).to.eql(\"UPH_PRD_HFF\");",
									"});",
									"",
									"pm.test(\"Provider ORG/ENV/APP (m_hsid) is UPP_PRD_EPC\", function() {",
									"    pm.expect(jsonData.data.m_hsid).to.eql(\"UPP_PRD_EPC\");",
									"});",
									"",
									"pm.test(\"Provider Id Type (m_pit) is EPI\", function() {",
									"    pm.expect(jsonData.data.m_pit).to.eql(\"EPI\");",
									"});",
									"",
									"pm.test(\"Audience (aud) is https://prod-moxehealth.com\", function() {",
									"    pm.expect(jsonData.data.aud).to.eql(\"https://prod-moxehealth.com\");",
									"});",
									"",
									"pm.test(\"Issuer (iss) is convergence-api\", function() {",
									"    pm.expect(jsonData.data.iss).to.eql(\"convergence-api\");",
									"});",
									"",
									"pm.test(\"Organization (m_org) is upmc\", function() {",
									"    pm.expect(jsonData.data.m_org).to.eql(\"upmc\");",
									"});"
